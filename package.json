        "eslint": "^4.14.0",
        "eslint-plugin-vue": "^4.0.1",
        "laravel-mix": "^2.*",
        "lodash": "^4.17.4"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "js-cookie": "^2.2.0",
        "vee-validate": "^2.0.3",
        "vue": "^2.5.13",
        "vue-router": "^3.0.1",
        "vuetify": "^1.0.0-beta.6",
        "vuex": "^3.0.1",
        "vuex-router-sync": "^5.0.0"
    }
